<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>TraitementCode</servlet-name>
        <servlet-class>controlleur.TraitementCode</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AffichageTransaction</servlet-name>
        <servlet-class>controlleur.AffichageTransaction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TraitementOperation</servlet-name>
        <servlet-class>controlleur.TraitementOperation</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProprieteController</servlet-name>
        <servlet-class>controlleur.ProprieteController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertionPropriete</servlet-name>
        <servlet-class>controlleur.InsertionPropriete</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TerrainController</servlet-name>
        <servlet-class>controlleur.TerrainController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AfficheMap</servlet-name>
        <servlet-class>controlleur.AfficheMap</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TraitementCode</servlet-name>
        <url-pattern>/TraitementCode</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AffichageTransaction</servlet-name>
        <url-pattern>/AffichageTransaction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TraitementOperation</servlet-name>
        <url-pattern>/TraitementOperation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProprieteController</servlet-name>
        <url-pattern>/ProprieteController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertionPropriete</servlet-name>
        <url-pattern>/InsertionPropriete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TerrainController</servlet-name>
        <url-pattern>/TerrainController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AfficheMap</servlet-name>
        <url-pattern>/AfficheMap</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
